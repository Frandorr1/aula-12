let objeto = {
    direção:'Roxann Dawson',
    nome:'O Milagre Da Fé',
    ano: 2019,
    elenco:['ChrissyMetz', 'MarcelRuiz', 'Mike Colter'],
    javiu: Boolean(true)
    

}
